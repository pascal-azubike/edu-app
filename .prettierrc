{
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"proseWrap": "always",
	"useTabs": true,
	"tabWidth": 2,
	"endOfLine": "lf",
	"overrides": [
		{
			"files": ["*.yml", "*.yaml"],
			"options": {
				"proseWrap": "preserve"
			}
		}
	]
}
